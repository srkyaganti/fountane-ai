{
  "name": "@./realtime-service",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "proto:generate": "pbjs -t static-module -w commonjs -o ./src/generated/realtime.js ../../../libs/core/proto/realtime.proto && pbts -o ./src/generated/realtime.d.ts ./src/generated/realtime.js"
  },
  "nx": {
    "name": "realtime-service",
    "targets": {
      "build": {
        "executor": "nx:run-commands",
        "options": {
          "command": "webpack-cli build",
          "args": [
            "--node-env=production"
          ]
        },
        "configurations": {
          "development": {
            "args": [
              "--node-env=development"
            ]
          }
        }
      },
      "serve": {
        "continuous": true,
        "executor": "@nx/js:node",
        "defaultConfiguration": "development",
        "dependsOn": [
          "build"
        ],
        "options": {
          "buildTarget": "realtime-service:build",
          "runBuildTargetDependencies": false
        },
        "configurations": {
          "development": {
            "buildTarget": "realtime-service:build:development"
          },
          "production": {
            "buildTarget": "realtime-service:build:production"
          }
        }
      }
    },
    "tags": [
      "scope:backend",
      "type:service"
    ]
  },
  "dependencies": {
    "axios": "^1.10.0",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "@types/uuid": "^10.0.0"
  }
}
